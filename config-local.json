{
    "contents": "./source/contents",
    "templates": "./source/templates",
    "jade": {
        "pretty": true
    },
    "browserify": {
        "ignore": [
            "./source/contents/vendor/*"
        ]
    },
    "node-sass": {
        "includePaths": [
            "./source/contents/css"
        ]
    },
    "locals": {
        "apiHost": "localhost",
        "apiPort": 3333,
        "apiProtocol": "http"
    },
    "require": {

    },    
    "plugins": [
        "./node_modules/wintersmith-coffee",
        "./node_modules/wintersmith-dot-sass"
    ],
    "ignore": [
        "*.coffee"
    ],
    "output": "./build",
    "baseUrl": "/",
    "hostname": "",
    "port": 1337
}
