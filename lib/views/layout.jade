		script(type='application/javascript', src='/app/vendor/ember-data.js')
		script(type='application/javascript', src='/app/vendor/ember-auth.js')
		script(type='application/javascript', src='/app/lib/app.js')
		script(type='application/javascript', src='/app/lib/models.js')
		script(type='application/javascript', src='/app/lib/views.js')
		script(type='application/javascript', src='/app/lib/controllers.js')
		script(type='application/javascript', src='/app/lib/router.js')
