html
	head(lang='en-US')
		title Beelancer | Social Project Management
		link(rel='stylesheet', type='text/css', href='/app/css/animate.min.css')
		link(rel='stylesheet', type='text/css', href='/app/css/jquery-ui-1.9.2.custom.min.css')
		link(rel='stylesheet', type='text/css', href='/app/css/jquery-ui-timepicker-addon.css')
		link(rel='stylesheet', type='text/css', href='/app/css/fonts.css')
		link(rel='stylesheet', type='text/css', href='/app/css/main.css')
		link(rel='stylesheet', type='text/css', href='/app/css/login.css')
		script(type='application/javascript', src='/socket.io/socket.io.js')
		//- script(type='application/javascript', src='/app/vendor/require.js')
		if google_analytics
			include ./includes/google-analytics.html
		script(type='application/javascript')
			| localStorage.setItem('api_url', '#{api_url}');
